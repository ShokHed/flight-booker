<h4>'Results:'</h4>
<div id="search_results">
  <%= form_with url: '/flights', method: :get  do |f| %>
    <% if @search_results %>
      <%= f.label :selected_flight %>
      <%# <%= f.collection_radio_buttons(:selected_flight, :flight_id, @search_results, :id, :search_results_display) %> %>
      <%= f.collection_radio_buttons(:flight_id, @search_results, :id, :search_results_display) %>

    <% else %>
      <p>'Select search parameters'</p>
    <% end %>
  <% end %>
</div>